{
  "name": "Register Person",
  "description": "During context day => if see a person, register in the next step, Only if see a person, register in the next step",
  "context": [
    "day"
  ],
  "contract": {
    "assumptions": [
      {
        "ltl_value": "G(F(person))"
      }
    ],
    "guarantees": [
      {
        "pattern": {
          "name": "BoundDelay",
          "arguments": [
            {
              "name": "trigger",
              "format": "value",
              "type": "any",
              "value": "person"
            },
            {
              "name": "reaction",
              "format": "value",
              "type": "any",
              "value": "register"
            }
          ]
        },
        "ltl_value": "G(person -> register)"
      }
    ]
  }
}